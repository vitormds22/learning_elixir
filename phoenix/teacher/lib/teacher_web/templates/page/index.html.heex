<section class="row">
  <article class="column">
    <h1>Oban tests</h1>

    <h3>Hard working app</h3>
    <%= form_for @changeset, Routes.page_path(@conn, :create), fn f -> %>
      <label>
        Name: <%= text_input f, :name %>
        <%= error_tag(f, :name) %>
      </label>

      <label>
        Age: <%= select f, :age, 18..100 %>
        <%= error_tag(f, :age) %>
      </label>
      <%= submit "Do more work" %>
    <% end %>
  </article>
</section>
